#include <stdio.h>

int main() {
int num;

printf("Enter a number between 2 and 14: ");
scanf("%d", &num);

switch (num) {
case 2:
printf("II\n");
break;
case 3:
printf("III\n");
break;
case 4:
printf("IV\n");
break;
case 5:
printf("V\n");
break;
case 6:
printf("VI\n");
break;
case 7:
printf("VII\n");
break;
case 8:
printf("VIII\n");
break;
case 9:
printf("IX\n");
break;
case 10:
printf("X\n");
break;
case 11:
printf("XI\n");
break;
case 12:
printf("XII\n");
break;
case 13:
printf("XIII\n");
break;
case 14:
printf("XIV\n");
break;
default:
printf("Error: Number out of range\n");
}

return 0;
}
